{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "always"
        },
    },
    "python.analysis.autoImportCompletions": false,
    "cSpell.words": [
        "endregion",
        "KHTML",
        "larchseed",
        "loccon",
        "Referer",
        "sqlalchemy"
    ]
}